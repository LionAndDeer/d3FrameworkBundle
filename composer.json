{
  "name": "liondeer/framework",
  "description": "L&D - Lion and Deer GmbH Framework for d.velop ECM",
  "license": "MIT",
  "type": "symfony-bundle",
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^2.0",
    "symfony/console": "^6.2.1",
    "symfony/dotenv": "^6.2.1",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "^6.2.1",
    "symfony/http-client": "^6.2.1",
    "symfony/maker-bundle": "*",
    "symfony/property-access": "^6.2.1",
    "symfony/property-info": "^6.2.1",
    "symfony/runtime": "^6.2.1",
    "symfony/security-bundle": "^6.2.1",
    "symfony/serializer": "^6.2.1",
    "symfony/translation": "^6.2.1",
    "symfony/twig-bundle": "^6.2.1",
    "symfony/yaml": "^6.2.1",
    "symfony/monolog-bundle": "^3.8"
  },
  "require-dev": {
    "symfony/stopwatch": "^6.2.1",
    "symfony/web-profiler-bundle": "^6.2.1"
  },
  "autoload": {
    "psr-4": {
      "Liondeer\\Framework\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Liondeer\\Framework\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "L & D - Lion & Deer GmbH",
      "email": "support@liondeer.de"
    },
    {
      "name": "Liondeer Community",
      "homepage": "https://github.com/orgs/LionAndDeer/people"
    }
  ],
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
