{
  "name": "liondeer/framework",
  "description": "Testing Bundles",
  "license": "MIT",
  "type": "symfony-bundle",
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.14",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpstan/phpdoc-parser": "^1.15",
    "sensio/framework-extra-bundle": "^6.1",
    "symfony/asset": "6.2.*",
    "symfony/console": "6.2.*",
    "symfony/doctrine-messenger": "6.2.*",
    "symfony/dotenv": "6.2.*",
    "symfony/expression-language": "6.2.*",
    "symfony/flex": "^2",
    "symfony/form": "6.2.*",
    "symfony/framework-bundle": "6.2.*",
    "symfony/http-client": "6.2.*",
    "symfony/intl": "6.2.*",
    "symfony/mailer": "6.2.*",
    "symfony/mime": "6.2.*",
    "symfony/monolog-bundle": "^3.0",
    "symfony/notifier": "6.2.*",
    "symfony/process": "6.2.*",
    "symfony/property-access": "6.2.*",
    "symfony/property-info": "6.2.*",
    "symfony/runtime": "6.2.*",
    "symfony/security-bundle": "6.2.*",
    "symfony/serializer": "6.2.*",
    "symfony/string": "6.2.*",
    "symfony/translation": "6.2.*",
    "symfony/twig-bundle": "6.2.*",
    "symfony/validator": "6.2.*",
    "symfony/web-link": "6.2.*",
    "symfony/yaml": "6.2.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Liondeer\\Framework\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Liondeer\\Framework\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "L & D - Lion & Deer GmbH",
      "email": "support@liondeer.de"
    },
    {
      "name": "Liondeer Community",
      "homepage": "https://github.com/orgs/LionAndDeer/people"
    }
  ],
  "extra": {
    "symfony": {
      "endpoint": [
        "https://api.github.com/repos/LionAndDeer/recipes/contents/index.json",
        "flex://defaults"
      ]
    }
  }
}